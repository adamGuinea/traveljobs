{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-moment": "^0.9.2",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "redux": "^4.0.2",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "watch:sass": "node-sass ./src/scss/style.scss ./src/css/style.min.css -w",
    "devserver": "live-server",
    "startcss": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass ./src/scss/style.scss ./src/css/comp.css",
    "concat:css": "concat -o ./src/css/concat.css ./src/css/comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 5 versions' ./src/css/concat.css -o ./src/css/prefix.css",
    "compress:css": "node-sass ./src/css/prefix.css ./src/css/style.min.css -output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5000",
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "concat": "^1.0.3",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3"
  }
}
